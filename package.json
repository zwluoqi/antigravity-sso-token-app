{
  "name": "antigravity-sso-token-app",
  "version": "1.7.1",
  "description": "Antigravity SSOToken Manager - 跨平台native应用用于管理Antigravity SSOToken",
  "homepage": "https://github.com/antigravity-team/antigravity-sso-token-app",
  "main": "src/main.js",
  "scripts": {
    "postinstall": "electron-rebuild",
    "rebuild": "electron-rebuild",
    "start": "electron .",
    "dev": "electron . --dev",
    "dev:utf8": "chcp 65001 && electron . --dev",
    "start:utf8": "chcp 65001 && electron .",
    "build": "electron-builder",
    "build-win": "electron-builder --win",
    "build-win-no-sign": "node build-win-no-sign.js",
    "build-mac": "electron-builder --mac",
    "build-mac-x64": "electron-builder --mac --x64",
    "build-mac-arm64": "electron-builder --mac --arm64",
    "build-mac-universal": "electron-builder --mac --universal",
    "build-linux": "electron-builder --linux",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build-all": "node build.js all",
    "build-windows": "node build.js win",
    "build-macos": "node build.js mac",
    "build-ubuntu": "node build.js linux",
    "test": "node test-app.js",
    "test-tokens": "node ../test-token-functions.js",
    "clean": "rimraf dist node_modules package-lock.json",
    "reinstall": "npm run clean && npm install"
  },
  "keywords": [
    "antigravity",
    "oauth",
    "token",
    "manager",
    "electron",
    "cross-platform"
  ],
  "author": {
    "name": "Antigravity Team",
    "email": "support@antigravity-team.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron/rebuild": "^3.6.0",
    "electron": "^27.3.11",
    "electron-builder": "^24.9.1"
  },
  "dependencies": {
    "better-sqlite3": "^11.0.0",
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.3",
    "node-fetch": "^2.7.0",
    "node-ssh": "^13.2.1",
    "plist": "^3.1.0"
  },
  "build": {
    "appId": "com.antigravity.antigravity-sso-token-manager",
    "productName": "Antigravity SSOToken Manager",
    "icon": "src/icon.png",
    "directories": {
      "output": "dist",
      "buildResources": "assets"
    },
    "forceCodeSigning": false,
    "nodeGypRebuild": false,
    "buildDependenciesFromSource": false,
    "npmRebuild": false,
    "asar": true,
    "asarUnpack": [
      "node_modules/better-sqlite3/**/*"
    ],
    "files": [
      "src/**/*",
      "assets/**/*",
      "node_modules/**/*",
      "!node_modules/.cache",
      "!node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!node_modules/*/{.github,.npmignore,.travis.yml,circle.yml}",
      "!node_modules/*/*.d.ts",
      "!node_modules/*/*.map",
      "!node_modules/.bin",
      "!node_modules/*/docs",
      "!node_modules/*/doc",
      "!node_modules/*/.eslintrc*",
      "!node_modules/*/.prettierrc*",
      "!node_modules/*/tsconfig.json",
      "!node_modules/*/tslint.json",
      "!node_modules/**/Makefile",
      "!node_modules/**/*.gyp",
      "!node_modules/**/*.gypi",
      "!node_modules/**/*.h",
      "!node_modules/**/*.c",
      "!node_modules/**/*.cc",
      "!node_modules/**/*.cpp",
      "!node_modules/**/*.o",
      "!node_modules/**/*.obj",
      "!node_modules/**/*.node.bak",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}"
    ],
    "extraResources": [
      {
        "from": "README.md",
        "to": "README.md"
      },
      {
        "from": "DEPLOYMENT.md",
        "to": "DEPLOYMENT.md"
      }
    ],
    "win": {
      "icon": "src/icon.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "publisherName": "Antigravity Team",
      "verifyUpdateCodeSignature": false,
      "sign": null
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Antigravity SSOToken Manager"
    },
    "mac": {
      "icon": "src/icon.png",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.developer-tools"
    },
    "linux": {
      "icon": "src/icon.png",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Development",
      "maintainer": "Antigravity Team <support@antigravity-team.com>"
    },
    "publish": null
  }
}